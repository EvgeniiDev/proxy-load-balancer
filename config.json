{
  "server": {
    "host": "0.0.0.0",
    "port": 8080,
    "keepalive_timeout": 75,
    "client_timeout": 30,
    "access_log": false
  },
  "connect_server": {
    "enabled": true,
    "host": "0.0.0.0",
