{
  "server": {
    "host": "0.0.0.0",
    "port": 8080
  },
  "ssl_cert": "cert.pem",
  "ssl_key": "key.pem",
  "proxies": [
    { "host": "127.0.0.1", "port": 9150 }
  ],
  "load_balancing_algorithm": "round_robin",
  "health_check_interval": 30,
  "connection_timeout": 30,
  "max_retries": 3,
  "proxy_rest_duration": 30,
  "overload_adaptive_multiplier": 0.5,
  "max_overload_multiplier": 3.0,
  "monitoring_interval": 10,
  "stats_log_interval": 60,
  "console_stats_interval": 30,
  "compact_console_stats": false
}